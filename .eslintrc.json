{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parserOptions": {
		"sourceType": "module"
	},
	"globals": {
		"__$$styleInject": true
	},
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"semi": [
			"error",
			"always"
		],
		"no-unused-vars": ["warn", { "varsIgnorePattern": "h", "ignoreRestSiblings": true }],
		"no-console": [
			"warn",
			{
				"allow": [
					"warn",
					"error"
				]
			}
		]
	}
}
